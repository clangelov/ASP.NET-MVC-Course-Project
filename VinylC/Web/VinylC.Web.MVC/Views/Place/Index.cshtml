@model IEnumerable<VinylC.Web.MVC.Models.Places.PlacesListViewModel>

<br />
<h2 class="text-center">See the coolest places among our Community</h2>
<script src="http://maps.google.com/maps/api/js" type="text/javascript"></script>
<div class="container">
    <div class="row">
        @{Html.RenderAction("GetPopularTags");}
    </div>
    <div class="row map">
        <div class="col-md-8 map" id="map_canvas">
            @*The map goes here*@
        </div>
        <div class="col-md-4">
            <div class="bs-component">
                <div class="list-group" style="overflow-y:auto">
                    @Html.Partial("_PlacesPartial", Model)
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {
    @Scripts.Render("~/bundles/maplogic")
}


