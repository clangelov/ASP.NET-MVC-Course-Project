@model IEnumerable<VinylC.Web.MVC.Models.Comments.CommentsViewModel>

<div class="actionBox">
    <ul class="commentList">
        @if (Model.Count() == 0)
        {
            <li class="text-center text-info">NO COMMENTS YET</li>
        }
        @foreach (var com in Model)
        {
            @Html.Partial("_SingleCommentPartial", com)
        }
    </ul>
</div>



